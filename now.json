{
  "version": 2,
  "name": "tresor-ticker-rust",
  "builds": [
    {
      "src": "src/main.rs",
      "use": "@now/rust"
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "src/main.rs"
    },
    {
      "src": "/",
      "methods": ["POST"],
      "dest": "src/main.rs"
    }
  ]
}
