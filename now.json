{
  "version": 2,
  "name": "tresor-ticker-rust",
  "builds": [
    {
      "src": "Cargo.toml",
      "use": "now-rust"
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "/tresor_ticker_rust"
    },
    {
      "src": "/",
      "methods": ["POST"],
      "dest": "/tresor_ticker_rust"
    }
  ]
}
